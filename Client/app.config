<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_AddEmployeeService" />
                <binding name="BasicHttpBinding_RetriveEmployeeService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:50294/EmployeeService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_AddEmployeeService"
                contract="EmployeeService.AddEmployeeService" name="BasicHttpBinding_AddEmployeeService" />
            <endpoint address="http://localhost:50294/EmployeeService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_RetriveEmployeeService"
                contract="EmployeeService.RetriveEmployeeService" name="BasicHttpBinding_RetriveEmployeeService" />
        </client>
    </system.serviceModel>
</configuration>